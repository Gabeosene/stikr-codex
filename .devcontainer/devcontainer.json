{
  "name": "Node.js Dev Container",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "VARIANT": "20-bullseye"
    }
  },
  "postCreateCommand": "corepack enable && npm install",
  "postAttachCommand": "npm run web",
  "forwardPorts": [19006],
  "portsAttributes": {
    "19006": {
      "label": "Expo Web"
    }
  }
}
